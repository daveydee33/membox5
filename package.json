  "bugs": {
    "url": "https://github.com/daveydee33/membox5/issues"
  },
  "homepage": "https://github.com/daveydee33/membox5#readme",
  "devDependencies": {
    "nodemon": "^2.0.2"
  },
  "dependencies": {
    "concurrently": "^5.1.0",
    "cookie-session": "^1.4.0",
    "dotenv": "^8.2.0",
    "express": "^4.18.2",
    "express-validator": "^6.4.0",
    "mongoose": "^5.12.0",
    "passport": "^0.4.1",
    "passport-google-oauth20": "^2.0.0"
  }
}
